{
  "schemaVersion": 1,
  "name": "KultKlient Legacy",
  "id": "kultklientlegacy",
  "version": "${version}",
  "description": "KultKlient Legacy is an advanced Minecraft utility client for Fabric forked from Meteor Client.",
  "authors": [
    "KultKollektive"
  ],
  "icon": "assets/kultklientlegacy/textures/icons/icon.png",
  "license": "GPL-3.0",
  "contact": {
    "homepage": "https://kultklient.github.io/",
    "discord": "https://kultklient.github.io//Discord",
    "issues": "https://kultklient.github.io//Discord"
  },
  "environment": "client",
  "entrypoints": {
    "client": [
      "kultklient.legacy.client.KultKlientLegacy"
    ]
  },
  "mixins": [
    "kultklientlegacy.mixins.json",
    "kultklientlegacy-canvas.mixins.json",
    "kultklientlegacy-sodium.mixins.json",
    "kultklientlegacy-indigo.mixins.json",
    "kultklientlegacy-baritone.mixins.json"
  ],
  "accessWidener": "kultklientlegacy.accesswidener",
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.website": "https://kultklient.github.io/",
        "modmenu.discord": "https://kultklient.github.io//Discord",
        "modmenu.issues": "https://kultklient.github.io//Discord",
        "modmenu.dupe": "https://www.youtube.com/watch?v=QDia3e12czc"
      }
    }
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=@mc_version@",
    "fabricloader": ">=0.12.11"
  },
  "breaks": {
    "optifabric": "*"
  }
}
