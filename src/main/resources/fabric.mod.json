{
  "schemaVersion": 1,
  "name": "KultKlient",
  "id": "kultklient",
  "version": "${version}",
  "description": "KultKlient is an advanced Minecraft utility client for Fabric forked from Meteor Client.",
  "authors": [
    "Matejko06"
  ],
  "icon": "assets/kultklient/textures/icons/icon.png",
  "license": "GPL-3.0",
  "contact": {
    "homepage": "https://kultklient.github.io",
    "discord": "https://kultklient.github.io/Discord",
    "issues": "https://kultklient.github.io/Discord"
  },
  "environment": "client",
  "entrypoints": {
    "client": [
      "kult.klient.KultKlient"
    ]
  },
  "mixins": [
    "kultklient.mixins.json",
    "kultklient-baritone.mixins.json",
    "kultklient-canvas.mixins.json",
    "kultklient-indigo.mixins.json",
    "kultklient-sodium.mixins.json"
  ],
  "accessWidener": "kultklient.accesswidener",
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.website": "https://kultklient.github.io",
        "modmenu.discord": "https://kultklient.github.io/Discord",
        "modmenu.issues": "https://kultklient.github.io/Discord"
      }
    }
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=@mc_version@",
    "fabricloader": ">=0.12.11"
  },
  "breaks": {
    "optifabric": "*"
  }
}
